{
  "name": "comptroller",
  "version": "0.2.3",
  "description": "A simple and lightweight tool to manage your monorepo.",
  "main": "src/comptroller.js",
  "directories": {
    "doc": "docs"
  },
  "dependencies": {
    "@zdychacek/detective": "^5.0.2",
    "babel-traverse": "^6.26.0",
    "builtin-modules": "^1.1.1",
    "dedent": "^0.7.0",
    "format-package": "^4.0.2",
    "fs-extra": "^4.0.2",
    "glob": "^7.1.2",
    "minimist": "^1.2.0"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "esdoc": "^1.0.3",
    "esdoc-ecmascript-proposal-plugin": "^1.0.0",
    "esdoc-node": "^1.0.2",
    "esdoc-standard-plugin": "^1.0.0",
    "mocha": "^4.0.1",
    "mocha-sinon": "^2.0.0",
    "nyc": "^11.3.0",
    "proxyquire": "^1.8.0",
    "sinon": "^4.1.1"
  },
  "bin": {
    "comp": "./src/cli.js"
  },
  "scripts": {
    "test": "nyc mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Aldlevine/comptroller.git"
